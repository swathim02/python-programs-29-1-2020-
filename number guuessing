import random
n = random.randrange(1, 99)
guess = int(input("Enter an integer from 1 to 100: "))
while n != "guess":
    print
    if guess < n:
        print ("guess is low")
        guess = int(input("Enter an integer from 1 to 100: "))
    elif guess > n:
        print ("guess is high")
        guess = int(input("Enter an integer from 1 to 100: "))
    else:
        print ("you guessed it")
        break
    print
    
    
    #input=aney number between 1 to 100
    #output=gussimg value
